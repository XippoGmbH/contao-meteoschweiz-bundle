{
    "name": "xippogmbh/meteoschweiz-bundle",
    "type": "contao-bundle",
    "description": "OSM Bundle for contao 4",
	"version": "v0.0.4",
    "homepage": "https://github.com/xippoGmbH/contao-meteoschweiz-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Aurelio Gisler",
            "homepage": "https://github.com/xippoGmbH"
        },
        {
            "name": "Contao Community",
            "homepage": "https://contao.org/contributors"
        }
    ],
    "require": {
        "php": "^7.0 | ^8.0",
        "contao/core-bundle": "^4.11",
        "symfony/framework-bundle": "^4.4 | ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "XippoGmbH\\MeteoschweizBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "XippoGmbH\\MeteoschweizBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XippoGmbH\\MeteoschweizBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/xippoGmbH/contao-meteoschweiz-bundle/issues",
        "source": "https://github.com/xippoGmbH/contao-meteoschweiz-bundle"
    }
}
